n  const signIn = useCallback(\n    async ({ username, password }: SignInCredentials) => {\n      await login({ variables: { username, password } });\n\n      if (data) {\n        setUserData(data.login);\n      }\n\n      history.push('/dashboard');\n      console.log(userData);\n    },\n    [login, userData, history, data],\n  );\n\n  return (\n    <AuthContext.Provider value={{ signIn, user: userData.user }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  return context;\n}\n\nexport { AuthProvider, useAuth };\n",["121","122","123","124","125"],"/home/kivel/Desktop/Projects/Link/web/src/hooks/index.tsx",[],"/home/kivel/Desktop/Projects/Link/web/src/routes/Route.tsx",[],"/home/kivel/Desktop/Projects/Link/web/src/hooks/apollo.tsx",["126"],"/home/kivel/Desktop/Projects/Link/web/src/App.tsx",[],["127","128","129","130","131"],"/home/kivel/Desktop/Projects/Link/web/src/utils/getValidationErrors.ts",[],"/home/kivel/Desktop/Projects/Link/web/src/gql/generated/graphql.tsx",["132","133","134","135","136","137","138","139","140","141","142","143","144","145"],{"ruleId":"146","replacedBy":"147"},{"ruleId":"148","replacedBy":"149"},{"ruleId":"150","replacedBy":"151"},{"ruleId":"152","replacedBy":"153"},{"ruleId":"154","replacedBy":"155"},{"ruleId":"156","severity":1,"message":"157","line":18,"column":15,"nodeType":"158","messageId":"159","endLine":18,"endColumn":26},{"ruleId":"156","severity":1,"message":"157","line":24,"column":3,"nodeType":"158","messageId":"159","endLine":24,"endColumn":14},{"ruleId":"156","severity":1,"message":"157","line":23,"column":28,"nodeType":"158","messageId":"159","endLine":23,"endColumn":39},{"ruleId":"156","severity":1,"message":"157","line":63,"column":7,"nodeType":"158","messageId":"159","endLine":63,"endColumn":18},{"ruleId":"156","severity":1,"message":"157","line":79,"column":7,"nodeType":"158","messageId":"159","endLine":79,"endColumn":18}